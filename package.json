{
    "name": "dev-extreme-admin",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "commit": "czg",
        "bootstrap": "pnpm install",
        "dev": "npx --max_old_space_size=4096 vite",
        "build": "vue-tsc --noEmit && vite build",
        "build-onle": "vite build",
        "build:no-cache": "pnpm clean:cache && npm run build",
        "preview": "pnpm run build && vite preview --port 4173",
        "preview:dist": "vite preview",
        "serve": "npx --max_old_space_size=4096 vite",
        "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
        "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
        "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
        "lint:ls-lint": "ls-lint",
        "lint:lint-staged": "lint-staged -c ./husky/lintstagedrc.js",
        "lint:lint-pretty": "pretty-quick --staged",
        "install:husky": "is-ci || husky install",
        "postinstall": "npm run install:husky",
        "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
        "clean:lib": "rimraf node_modules",
        "reinstall": "rimraf pnpm-lock.yaml && rimraf node_modules && npm run bootstrap",
        "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "dependencies": {
        "@types/node": "^20.2.5",
        "@vitejs/plugin-vue-jsx": "^3.0.1",
        "@vueuse/core": "^10.1.2",
        "axios": "^1.4.0",
        "conventional-changelog-cli": "^2.2.2",
        "crypto-js": "^4.1.1",
        "dayjs": "^1.11.7",
        "devextreme": "^22.2.6",
        "devextreme-vue": "^22.2.6",
        "dotenv": "^16.0.3",
        "install": "^0.13.0",
        "lodash-es": "^4.17.21",
        "monaco-editor": "^0.38.0",
        "nprogress": "^0.2.0",
        "pinia": "^2.1.3",
        "postcss-html": "^1.5.0",
        "qs": "^6.11.2",
        "stylelint-config-recommended": "^12.0.0",
        "vite-plugin-mkcert": "^1.15.0",
        "vite-plugin-purge-icons": "^0.9.2",
        "vue": "^3.3.4",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.2.2",
        "vue-types": "^5.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "@ls-lint/ls-lint": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "@vitejs/plugin-vue": "^4.2.3",
        "autoprefixer": "^10.4.14",
        "commitizen": "^4.3.0",
        "cz-git": "^1.6.1",
        "czg": "^1.6.1",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.14.1",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "less": "^4.1.3",
        "less-loader": "^11.1.1",
        "lint-staged": "^13.2.2",
        "postcss": "^8.4.24",
        "prettier": "^2.8.8",
        "pretty-quick": "^3.1.3",
        "rimraf": "^5.0.1",
        "stylelint": "^15.6.2",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-standard": "^33.0.0",
        "stylelint-order": "^6.0.3",
        "typescript": "^5.0.4",
        "vite": "^4.3.9",
        "vue-eslint-parser": "^9.3.0",
        "vue-tsc": "^1.6.5"
    }
}